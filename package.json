{
  "name": "dev-notebooks",
  "version": "v1.0.1",
  "description": "A developer notebooks app",
  "main": "./build/electron-main.js",
  "scripts": {
    "start-web": "node node_modules/.bin/webpack-dev-server --config ./config/webpack.config.dev",
    "pre-build": "rimraf build && rimraf dist && cpx 'src/!(web)/**/*.!(ts)' build",
    "build-web": "node ./config/build.js",
    "build-server": "tsc --build tsconfig.server.json",
    "build-electron": "npm run pre-build && npm run build-web && tsc --build tsconfig.electron.json",
    "lint:client": "tslint --project tsconfig.client.json",
    "lint:server": "tslint --project tsconfig.server.json",
    "pack-electron": "NODE_ENV=production npm run build-electron && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "release": "build"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/roblopz/dev-notebook"
  },
  "author": "https://github.com/roblopz",
  "license": "ISC",
  "build": {
    "appId": "com.dev-notebooks.app",
    "productName": "dev-notebooks",
    "files": [
      "./build/**/*"
    ],
    "publish": {
      "provider": "github"
    }
  },
